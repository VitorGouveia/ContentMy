{
  "name": "@contentmy/server",
  "description": "ContentMy Server",
  "author": "Vitor Gouveia <vitorneves.gouveia10@gmail.com>",
  "packageManager": "yarn@3.2.0",
  "main": "./dist/index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only --quiet --no-notify src/index.ts",
    "build": "tsnd scripts/esbuild.build.ts",
    "start": "node dist/index.js",
    "test": "jest --watch --verbose",
    "env": "gen-env-types --types-output src/@types/env.d.ts --example-env-path . --keep-comments"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.31",
    "gen-env-types": "^1.3.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dayjs": "^1.10.8",
    "esbuild": "^0.14.25",
    "esbuild-node-externals": "^1.4.1",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.3.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "nock": "^13.2.4",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
